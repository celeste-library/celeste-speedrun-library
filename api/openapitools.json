{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.2.0",
    "generators": {
      "server": {
        "generatorName": "python-flask",
        "inputSpec": "api.yaml",
        "output": "#{cwd}/../server/generated/",
        "additionalProperties": {
        }
      },
      "client": {
        "generatorName": "typescript-fetch",
        "inputSpec": "api.yaml",
        "output": "#{cwd}/../client/src/generated/",
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
